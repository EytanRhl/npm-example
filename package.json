{
  "name": "my-big-application",
  "version": "1.0.0",
  "description": "A large-scale application built with Node.js and React.",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "client": "webpack serve --config webpack.client.js --mode development",
    "server": "nodemon server/index.js",
    "build": "webpack --config webpack.client.js --mode production",
    "test": "jest",
    "lint": "eslint .",
    "prettify": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md}'",
    "prepare": "husky install"
  },
  "keywords": [
    "node",
    "react",
    "application"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.10.9",
    "cors": "^2.8.5",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "axios": "^0.21.1",
    "express-session": "^1.17.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "socket.io": "^3.0.3",
    "redux": "^4.0.5",
    "react-redux": "^7.2.4",
    "redux-thunk": "^2.3.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.2",
    "babel-loader": "^8.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "jest": "^26.6.0",
    "eslint": "^7.15.0",
    "prettier": "^2.2.1",
    "husky": "^7.0.2",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "webpack-merge": "^5.7.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/my-big-application.git"
  },
  "bugs": {
    "url": "https://github.com/username/my-big-application/issues"
  },
  "homepage": "https://github.com/username/my-big-application#readme"
